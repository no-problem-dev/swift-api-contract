 Output = [User]","}"],"syntax":"swift","type":"codeListing"},{"level":3,"text":"パスパラメータを持つエンドポイント","anchor":"%E3%83%91%E3%82%B9%E3%83%91%E3%83%A9%E3%83%A1%E3%83%BC%E3%82%BF%E3%82%92%E6%8C%81%E3%81%A4%E3%82%A8%E3%83%B3%E3%83%89%E3%83%9D%E3%82%A4%E3%83%B3%E3%83%88","type":"heading"},{"inlineContent":[{"type":"text","text":"パスパラメータは"},{"type":"codeVoice","code":"@PathParam"},{"type":"text","text":"でマークします。"}],"type":"paragraph"},{"code":["@Endpoint(.get, path: \":userId\")","struct GetUser {","    @PathParam var userId: String","    typealias Output = User","}"],"syntax":"swift","type":"codeListing"},{"level":3,"text":"クエリパラメータを持つエンドポイント","anchor":"%E3%82%AF%E3%82%A8%E3%83%AA%E3%83%91%E3%83%A9%E3%83%A1%E3%83%BC%E3%82%BF%E3%82%92%E6%8C%81%E3%81%A4%E3%82%A8%E3%83%B3%E3%83%89%E3%83%9D%E3%82%A4%E3%83%B3%E3%83%88","type":"heading"},{"inlineContent":[{"type":"text","text":"クエリパラメータは"},{"type":"codeVoice","code":"@QueryParam"},{"type":"text","text":"でマークします。"}],"type":"paragraph"},{"code":["@Endpoint(.get)","struct SearchUsers {","    @QueryParam var query: String","    @QueryParam var limit: Int?","    @QueryParam(\"page_size\") var pageSize: Int?  \/\/ カスタム名","","    typealias Output = [User]","}"],"syntax":"swift","type":"codeListing"},{"level":3,"text":"リクエストボディを持つエンドポイント","anchor":"%E3%83%AA%E3%82%AF%E3%82%A8%E3%82%B9%E3%83%88%E3%83%9C%E3%83%87%E3%82%A3%E3%82%92%E6%8C%81%E3%81%A4%E3%82%A8%E3%83%B3%E3%83%89%E3%83%9D%E3%82%A4%E3%83%B3%E3%83%88","type":"heading"},{"inlineContent":[{"text":"リクエストボディは","type":"text"},{"code":"@Body","type":"codeVoice"},{"text":"でマークします。","type":"text"}],"type":"paragraph"},{"code":["@Endpoint(.post)","struct CreateUser {","    @Body var body: CreateUserRequest","    typealias Output = User","}"],"syntax":"swift","type":"codeListing"},{"level":2,"text":"APIグループの定義","anchor":"API%E3%82%B0%E3%83%AB%E3%83%BC%E3%83%97%E3%81%AE%E5%AE%9A%E7%BE%A9","type":"heading"},{"inlineContent":[{"type":"text","text":"関連するエンドポイントは"},{"type":"codeVoice","code":"@APIGroup"},{"type":"text","text":"でグループ化できます。"}],"type":"paragraph"},{"code":["@APIGroup(path: \"\/v1\/users\", auth: .required)","enum UsersAPI {","    @Endpoint(.get)","    struct List {","        @QueryParam var limit: Int?","        typealias Output = [User]","    }","","    @Endpoint(.get, path: \":userId\")","    struct Get {","        @PathParam var userId: String","        typealias Output = User","    }","","    @Endpoint(.post)","    struct Create {","        @Body var body: CreateUserRequest","        typealias Output = User","    }","","    @Endpoint(.delete, path: \":userId\")","    struct Delete {","        @PathParam var userId: String","        typealias Output = EmptyOutput","    }","}"],"syntax":"swift","type":"codeListing"},{"level":2,"text":"エンドポイントの実行","anchor":"%E3%82%A8%E3%83%B3%E3%83%89%E3%83%9D%E3%82%A4%E3%83%B3%E3%83%88%E3%81%AE%E5%AE%9F%E8%A1%8C","type":"heading"},{"level":3,"text":"APIExecutorの実装","anchor":"APIExecutor%E3%81%AE%E5%AE%9F%E8%A3%85","type":"heading"},{"inlineContent":[{"text":"まず、","type":"text"},{"type":"codeVoice","code":"APIExecutor"},{"text":"プロトコルを実装したクライアントを作成します。","type":"text"}],"type":"paragraph"},{"code":["struct MyAPIClient: APIExecutor {","    let baseURL: String","    let session: URLSession","","    func execute<E: APIContract>(_ endpoint: E) async throws -> E.Output","    where E.Output: Decodable {","        let request = try endpoint.urlRequest(baseURL: baseURL)","        let (data, _) = try await session.data(for: request)","        return try JSONDecoder().decode(E.Output.self, from: data)","    }","","    func execute<E: APIContract>(_ endpoint: E) async throws","    where E.Output == EmptyOutput {","        let request = try endpoint.urlRequest(baseURL: baseURL)","        _ = try await session.data(for: request)","    }","}"],"syntax":"swift","type":"codeListing"},{"level":3,"text":"リクエストの実行","anchor":"%E3%83%AA%E3%82%AF%E3%82%A8%E3%82%B9%E3%83%88%E3%81%AE%E5%AE%9F%E8%A1%8C","type":"heading"},{"code":["